<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/dateLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Data:"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/dateLabelTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="15dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="data"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team1Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/dateLabel"
        android:text="Zespół 1/Gracz 1:"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team1LabelTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="15dp"
        android:layout_alignParentRight="true"
        android:layout_below="@id/dateLabelTV"
        android:text="team1"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team1ScoreLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/team1Label"
        android:text="Wynik:"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team1ScoreLabelTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="15dp"
        android:layout_alignParentRight="true"
        android:layout_below="@id/team1LabelTV"
        android:text="score1"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team2Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/team1ScoreLabel"
        android:text="Zespół 2/Gracz 2:"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team2LabelTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="15dp"
        android:layout_alignParentRight="true"
        android:layout_below="@id/team1ScoreLabelTV"
        android:text="team2"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team2ScoreLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/team2Label"
        android:text="Wynik:"
        android:textSize="@dimen/search_league_list_element_text_size"/>

    <TextView
        android:id="@+id/team2ScoreLabelTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="15dp"
        android:layout_alignParentRight="true"
        android:layout_below="@id/team2LabelTV"
        android:text="score2"
        android:textSize="@dimen/search_league_list_element_text_size"/>
</RelativeLayout>